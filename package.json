{
  "name": "tactile-weather",
  "version": "0.0.3",
  "type": "module",
  "private": false,
  "main": "./dist/tactile-weather.umd.js",
  "module": "./dist/tactile-weather.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/tactile-weather.es.js",
      "require": "./dist/tactile-weather.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build:lib": "tsc && vite build",
    "build:demo": "tsc && vite build --config vite.demo.config.ts",
    "preview": "vite preview",
    "predeploy": "npm run build:demo",
    "deploy": "gh-pages -d dist-demo"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "lucide-react": "^0.561.0"
  },
  "devDependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "gh-pages": "^6.3.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.4"
  }
}
